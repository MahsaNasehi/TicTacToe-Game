<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="550.0" minWidth="200.0" prefHeight="550.0" prefWidth="500.0" style="-fx-background-color: #1b1a1a; -fx-border-color: #ffffff; -fx-border-radius: 10px;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.workshop12.LogInController">
   <children>
      <Label layoutX="195.0" layoutY="31.0" text="Tic Tac Toe" textFill="WHITE">
         <font>
            <Font name="Impact" size="24.0" />
         </font>
      </Label>
      <Button layoutX="53.0" layoutY="186.0" mnemonicParsing="false" style="-fx-background-color: #1b1a1a; -fx-border-color: #ffffff; -fx-border-radius: 15px;" text="Username" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <TextField fx:id="name_text" alignment="CENTER" layoutX="122.0" layoutY="254.0" prefHeight="41.0" prefWidth="328.0" promptText="Enter your name" style="-fx-background-radius: 15px;">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </TextField>
      <Button fx:id="go_butt" layoutX="286.0" layoutY="357.0" mnemonicParsing="false" onAction="#clickGo" prefHeight="34.0" prefWidth="82.0" style="-fx-background-color: #1b1a1a; -fx-border-color: #ffffff; -fx-border-radius: 15px;" text="Let's go" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="back_butt" layoutX="143.0" layoutY="357.0" mnemonicParsing="false" onAction="#clickBack" prefHeight="34.0" prefWidth="82.0" style="-fx-background-color: #1b1a1a; -fx-border-color: #ffffff; -fx-border-radius: 15px;" text="go back" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="name_alert_label" layoutX="129.0" layoutY="296.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="311.0" style="-fx-background-color: #1b1a1a;" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
