<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="550.0" minWidth="200.0" prefHeight="550.0" prefWidth="500.0" style="-fx-background-color: #1b1a1a; -fx-border-color: #ffffff; -fx-border-radius: 10px;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.workshop12.StartController">
   <children>
      <Label layoutX="175.0" layoutY="84.0" text="Tic Tac Toe" textFill="WHITE">
         <font>
            <Font name="Impact" size="33.0" />
         </font>
      </Label>
      <Button fx:id="start_butt" layoutX="216.0" layoutY="254.0" mnemonicParsing="false" onAction="#clickStart" style="-fx-background-color: #1b1a1a; -fx-border-color: #ffffff; -fx-border-radius: 20px;" text="Start" textFill="WHITE">
         <font>
            <Font size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="exit_butt" layoutX="222.0" layoutY="352.0" mnemonicParsing="false" onAction="#clickExit" style="-fx-background-color: #1b1a1a; -fx-border-radius: 20px; -fx-border-color: #ffffff;" text="Exit" textFill="WHITE">
         <font>
            <Font size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
